<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    
    <bean class="com.autowiring.autowiring1_project.Certificate" name="certi">
    <property name="cname" value="spring and hibernate"></property>
    <property name="cduration" value="2.5"></property>
    </bean>
    <bean class="com.autowiring.autowiring1_project.Student" name="student" autowire="byName">
    <property name="sname" value="rajeshwar"></property>
    <property name="sid" value="123"></property>
    <property name="branch" value="computer science"></property>
    </bean>
   
</beans>